{
  "version": 1,
  "projects": {
    "castmate-api": {
      "root": "apps/castmate-api",
      "sourceRoot": "apps/castmate-api/src",
      "projectType": "application",
      "prefix": "castmate-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/castmate-api",
            "main": "apps/castmate-api/src/main.ts",
            "tsConfig": "apps/castmate-api/tsconfig.app.json",
            "assets": ["apps/castmate-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/castmate-api/src/environments/environment.ts",
                  "with": "apps/castmate-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "castmate-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/castmate-api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/castmate-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "castmate-client": {
      "root": "apps/castmate-client",
      "sourceRoot": "apps/castmate-client",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/castmate-client",
            "outputPath": "dist/apps/castmate-client"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "castmate-client:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "castmate-client:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "castmate-client:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/castmate-client/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/castmate-client"],
          "options": {
            "jestConfig": "apps/castmate-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "castmate-client-e2e": {
      "root": "apps/castmate-client-e2e",
      "sourceRoot": "apps/castmate-client-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/castmate-client-e2e/cypress.json",
            "tsConfig": "apps/castmate-client-e2e/tsconfig.e2e.json",
            "devServerTarget": "castmate-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "castmate-client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/castmate-client-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "auth": {
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/auth/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/auth/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "prisma": {
      "root": "libs/prisma",
      "sourceRoot": "libs/prisma/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/prisma/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/prisma/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "castmate-api",
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  }
}
